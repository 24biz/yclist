<script type="text/javascript">
function clearMe(formfield){
if (formfield.defaultValue==formfield.value)
formfield.value = ""
}
function returnMe(formfield){
if (formfield.value=="")
formfield.value = formfield.defaultValue
}
$(document).ready(function() {
$('.panelButton').click(function() {
// hide all panels
$('.panel').hide()
// un-highlight all as well
$('.panelButton').removeClass('bluebuttonhighlighted');
// show the selected panel
var selectedPanel = $(this).attr('panelTarget');
$('#' + selectedPanel).show();
// highlight the selected button
$(this).addClass('bluebuttonhighlighted');
// Resize black background to fit
var preferredHeight = parseInt($(this).attr('preferredHeight'));
$('#contentBackground').height(preferredHeight);
});
});
</script>
