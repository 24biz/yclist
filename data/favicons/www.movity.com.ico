<script type="text/javascript">
function submitBetaForm(betaEmail)
{
$('#signup').hide();
$('#postsignup').show();
alert(betaEmail);
}
function storeBetaEmail(betaEmail)
{
var ajax_url = "/beta/store_beta_email/";
$.post(ajax_url,{ email : betaEmail }, function(j){
if(j.status == 'success')
{
$('#signupInputPanel').hide();
$('#signupInputPanelCompleted').show();
}
},
"json");
}
</script>
